{
  "env": "qa",
  "web3": {
    "fuseProvider": "https://rpc.fuse.io"
  },
  "api": {
    "auth": {
      "domain": {
        "name": "Fuse Studio QA"
      }
    },
    "url": {
      "main": "https://studio-qa.fuse.io/api/v1",
      "ropsten": "https://studio-qa-ropsten.fuse.io/api/v1",
      "default": "https://studio-qa.fuse.io/api/v1"
    },
    "v2": {
      "url": {
        "main": "https://studio-qa.fuse.io/api/v2",
        "ropsten": "https://studio-qa-ropsten.fuse.io/api/v2"
      }
    },
    "graph": {
      "url": "https://graph-qa.fuse.io/subgraphs/name/fuseio",
      "subgraphs": {
        "entities": "/fuse-entities-qa"
      }
    }
  },
  "reactGA": {
    "gaOptions": {
      "testMode": false
    }
  },
  "ipfsProxy": {
    "urlBase": "https://ipfs-proxy-qa.fuse.io/api"
  },
  "torus": {
    "env": "staging"
  }
}