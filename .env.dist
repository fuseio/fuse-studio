# Project name - affects containers and network name
PROJECT_NAME=cln-community

# User/group ID management
UID=1000
GID=1000

# Application parameters
COMMUNITY_API_SECRET=somerandomhash
COMMUNITY_COLU_HOST=${PROJECT_NAME}-server
COMMUNITY_COLU_PORT=9000
COMMUNITY_APP_ENV=development
COMMUNITY_APP_PORT=3000
# App - Mandrill API endpoint and address to recv messages
COMMUNITY_MANDRILL_API=
COMMUNITY_MANDRILL_SEND_TO=
# App - Mailchimp API key and list to use
COMMUNITY_MAILCHIMP_API_BASE=
COMMUNITY_MAILCHIMP_LIST_NAME=
# App - Amazon bucket to retrieve static data from
COMMUNITY_AMAZON_BUCKET_API=

# Mongo setup
