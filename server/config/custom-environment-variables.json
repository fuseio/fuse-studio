{
  "api": {
    "allowCors": "COMMUNITY_API_ALLOW_CORS",
    "secret": "COMMUNITY_API_SECRET",
    "tokenExpiresIn": "COMMUNITY_TOKEN_EXPIRES_IN",
    "port": "PORT"
  },
  "aws": {
    "secrets": {
      "secretId": "COMMUNITY_AWS_SECRET_ID",
       "manager": {
        "region": "COMMUNITY_AWS_SECRET_MANAGER_REGION"
      }
    }
  },
  "ipfsProxy": {
    "urlBase": "COMMUNITY_IPFS_PROXY_URL_BASE"
  },
  "network": {
    "misc": {
      "pageSize": "COMMUNITY_WEB3_PAGE_SIZE"
    },
    "home": {
      "provider": "COMMUNITY_NETWORK_HOME_PROVIDER",
      "gasPrice": "COMMUNITY_NETWORK_HOME_GAS_PRICE"
    },
    "foreign": {
      "apiKey": "COMMUNITY_WEB3_API_KEY",
      "name": "COMMUNITY_WEB3_NETWORK",
      "gasStation": "COMMUNITY_WEB3_GAS_STATION"
    }
  },
  "mongo": {
    "uri": "COMMUNITY_MONGO_URI",
    "debug": "COMMUNITY_MONGO_DEBUG",
    "options": {
      "user": "COMMUNITY_MONGO_USER",
      "pass": "COMMUNITY_MONGO_PASS",
      "authSource": "COMMUNITY_MONGO_AUTH_SOURCE"
    }
  },
  "mail": {
    "supportAddress": "COMMUNITY_MAIL_SUPPORT_ADDRESS",
    "sendgrid": {
      "apiKey": "COMMUNITY_MAIL_SENDGRID_API_KEY",
      "listId": "COMMUNITY_MAIL_SENDGRID_LIST_ID",
      "templates": {
        "welcome": "COMMUNITY_MAIL_SENDGRID_TEMPLATES_WELCOME"
      }
    }
  },
  "secrets": {
    "accounts": {
      "seed": "COMMUNITY_SECRETS_ACCOUNTS_SEED"
    },
    "fuse": {
      "bridge": {
        "privateKey": "COMMUNITY_SECRETS_FUSE_BRIDGE_PRIVATE_KEY"
      }
    }
  },
  "agenda": {
    "tasks": {
      "deploy": {
        "concurrency": "COMMUNITY_AGENDA_TASKS_DEPLOY_CONCURRENCY"
      }
    }
  }
}
