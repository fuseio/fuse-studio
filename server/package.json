{
  "name": "fuse-studio-backend",
  "version": "0.1.0",
  "description": "backend for the Fuse Studio DApp",
  "main": "app.js",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "nodemon --trace-warnings ./src/app.js",
    "debug": "node --inspect --trace-warnings ./src/app.js",
    "debug-brk": "node --inspect-brk --trace-warnings ./src/app.js",
    "doc:v1": "apidoc -c src/routes/api/v1 -i src/routes/api/v1 -o docs/api/v1 && apidoc-markdown -p docs/api/v1 -o docs/api-v1.md",
    "doc:v2": "apidoc -c src/routes/api/v2 -i src/routes/api/v2 -o docs/api/v2 && apidoc-markdown -p docs/api/v2 -o docs/api-v2.md",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuseio/fuse-studio.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "artifacts",
      "assert",
      "contract"
    ],
    "ignore": [
      "docs"
    ]
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services",
    "@tasks": "./src/tasks",
    "@handlers": "./src/handlers"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@fuse/entities-contracts": "../@fuse/entities-abi",
    "@fuse/roles": "../@fuse/roles",
    "@fuse/token-factory-contracts": "../@fuse/token-factory-abi",
    "@portis/web3": "^2.0.0-beta.54",
    "@sendgrid/client": "^6.3.0",
    "@sendgrid/mail": "^6.3.1",
    "agenda": "^2.0.2",
    "async": "^2.6.1",
    "aws-sdk": "^2.405.0",
    "bignumber.js": "^8.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "2.7.1",
    "debug": "2.6.9",
    "eth-sig-util": "^2.1.1",
    "ethereumjs-wallet": "^0.6.3",
    "ethers": "^4.0.40",
    "express": "4.16.3",
    "express-async-errors": "^2.1.2",
    "express-jwt": "^5.3.1",
    "express-paginate": "^1.0.0",
    "firebase-admin": "^8.9.2",
    "graphql-request": "^1.8.2",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.13",
    "methods": "1.1.2",
    "module-alias": "^2.1.0",
    "moment": "^2.23.0",
    "mongoose": "^5.4.4",
    "mongoose-paginate-v2": "^1.3.9",
    "morgan": "^1.9.1",
    "multer": "^1.3.1",
    "node-slackr": "^1.0.1",
    "portis": "^1.3.5",
    "request": "^2.88.0",
    "request-ip": "^2.1.3",
    "request-promise-native": "^1.0.7",
    "twilio": "^3.37.0",
    "validator": "^10.10.0",
    "web3": "^2.0.0-alpha.1",
    "web3-utils": "^1.2.1",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "@rigwild/apidoc-markdown": "^2.0.2",
    "apidoc": "^0.17.7",
    "nodemon": "^1.18.11",
    "standard": "^12.0.1"
  }
}
